{
  "cli": {
    "version": ">= 12.6.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "EXPO_PUBLIC_API_URL",
        "EXPO_PUBLIC_FUNDING_URL": "EXPO_PUBLIC_FUNDING_URL",
        "EXPO_PUBLIC_EAS_SCHEMA_UID": "EXPO_PUBLIC_EAS_SCHEMA_UID",
        "EXPO_PUBLIC_NETWORK_PROVIDER": "EXPO_PUBLIC_NETWORK_PROVIDER",
        "EXPO_PUBLIC_NETWORK_SCAN": "EXPO_PUBLIC_NETWORK_SCAN"
      }
    },
    "preview": {
      "ios": {
        "simulator": true
      },
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "developmentClient": false,
      "env": {
        "EXPO_PUBLIC_API_URL": "EXPO_PUBLIC_API_URL",
        "EXPO_PUBLIC_FUNDING_URL": "EXPO_PUBLIC_FUNDING_URL",
        "EXPO_PUBLIC_EAS_SCHEMA_UID": "EXPO_PUBLIC_EAS_SCHEMA_UID",
        "EXPO_PUBLIC_NETWORK_PROVIDER": "EXPO_PUBLIC_NETWORK_PROVIDER",
        "EXPO_PUBLIC_NETWORK_SCAN": "EXPO_PUBLIC_NETWORK_SCAN"
      }
    },
    "production-apk": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "developmentClient": false
    }
  },
  "submit": {
    "production": {}
  }
}
