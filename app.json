{
  "expo": {
    "name": "Enaleia",
    "slug": "enaleia",
    "version": "0.1.2",
    "newArchEnabled": true,
    "owner": "pollen-labs",
    "orientation": "portrait",
    "icon": "./assets/images/splash-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "ios": {
      "jsEngine": "jsc",
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.enaleia",
      "infoPlist": {
        "UIViewControllerBasedStatusBarAppearance": false,
        "UIBackgroundModes": [
        ],
        "NSCameraUsageDescription": "The app need access to your camera to scan QR codes.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "userInterfaceStyle": "light",
      "icon": "./assets/images/splash-icon.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#F6F4F2"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.anonymous.enaleia",
      "userInterfaceStyle": "light"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#F6F4F2",
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 300
        }
      ],
      "expo-router",
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Your location is used to strengthen the authenticity of your action attestation."
        }
      ],
      [
        "expo-camera",
        {
        }
      ],
      "expo-font",
      "expo-localization",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "f01ad84b-7116-4e30-bbe5-6bf3045433b1"
      }
    }
  }
}
